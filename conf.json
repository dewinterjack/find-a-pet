{
    "tags": {
        "allowUnknownTags": true
    },
      "source": {
          "include" : [
            "./imports",
            "./client",
            "./docs/README.md"]
      },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "docdash": {
        "static": [true],
        "sort": [true]
    }
}
